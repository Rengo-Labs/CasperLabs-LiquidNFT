  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.7.14",
    "dotenv": "^10.0.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "casper-js-sdk": "2.5.2",
    "ts-results": "^3.3.0"
  }
}
